# テスト用環境変数設定
# 統合テスト実行時に使用される設定

# Supabase Test Configuration
SUPABASE_URL=https://test-project.supabase.co
SUPABASE_ANON_KEY=your-test-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-test-service-role-key-here

# Test Database Configuration
TEST_DB_HOST=localhost
TEST_DB_PORT=5432
TEST_DB_NAME=yata_test
TEST_DB_USER=test_user
TEST_DB_PASSWORD=test_password

# Test Environment Settings
FLUTTER_TEST_TIMEOUT=30000
INTEGRATION_TEST_TIMEOUT=60000
PERFORMANCE_TEST_ITERATIONS=100

# Feature Flags for Testing
ENABLE_MOCK_SERVICES=true
ENABLE_TEST_DATA_CLEANUP=true
ENABLE_PERFORMANCE_MONITORING=true
ENABLE_MEMORY_PROFILING=true
